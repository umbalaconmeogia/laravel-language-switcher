{
    "name": "umbalaconmeogia/laravel-language-switcher",
    "description": "A simple CSS framework independent Laravel package for language switching",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tran Trung Thanh",
            "email": "umbalaconmeogia@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": ">=11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Umbalaconmeogia\\LanguageSwitcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umbalaconmeogia\\LanguageSwitcher\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Umbalaconmeogia\\LanguageSwitcher\\LanguageSwitcherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "multilingual",
        "language",
        "localization",
        "i18n",
        "language-switcher"
    ]
} 